{
  "version": 2,
  "builds": [
    {
      "src": "server/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "client/build" }
    }
  ],
  "routes": [
    {
      "src": "/server/(.*)",
      "dest": "/server/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
